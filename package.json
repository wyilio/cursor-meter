{
  "name": "cursor-meter",
  "displayName": "Cursor Meter",
  "description": "Cursor usage information right in your IDE",
  "version": "0.1.0",
  "publisher": "wyilio",
  "icon": "images/cursor-meter.jpeg",
  "repository": {
    "type": "git",
    "url": "https://github.com/wyilio/cursor-meter"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.105.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-meter.refresh",
        "title": "Cursor Meter: Refresh Usage",
        "icon": "$(refresh)"
      },
      {
        "command": "cursor-meter.setToken",
        "title": "Cursor Meter: Change WorkOS Token",
        "category": "Cursor Meter"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
